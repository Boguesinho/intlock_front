<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.intlok">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:usesCleartextTraffic="true"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Intlok">
        <activity android:name=".activity_comentarios"></activity>
        <activity android:name=".activity_registromain"></activity>
        <activity android:name=".activity_registrocontinuacion"></activity>
        <activity android:name=".activity_main"></activity>

        <activity android:name=".activity_nuevo_post" />
        <activity
            android:name=".activity_loginmain"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>